$texturegroup "pyro_Skins"
{
	{ "pyro_red"          "pyro_head"               "pyro_goggles"       "pyro_gibs_red"     "pyro_gibs_head"    	  }
	{ "pyro_blue"         "pyro_head"               "pyro_goggles"       "pyro_gibs_blue"    "pyro_gibs_head"    	  }
